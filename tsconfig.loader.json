{
  "compilerOptions": {
    "outDir": "./dist",
    "module": "CommonJS",
    "target": "es2020",
    "lib": [
      "esnext",
    ],
    "sourceMap": false,
    // "allowJs": true,
    "jsx": "react",
    "skipLibCheck": true,
    "moduleResolution": "node",
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "rootDir": ".",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "incremental": true,
  },
  "include": [
    "loader/**/*",
  ],
  "exclude": [
    "node_modules/**/*",
    "node_modules/@types/**/*",
  ]
}